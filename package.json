{
  "name": "target-words",
  "description": "Mobile-platform word search game",
  "version": "0.9.0",
  "main": "src/",
  "license": "MIT",
  "author": {
    "name": "Wolf Hatch"
  },
  "engines": {
    "node": ">= 7.9.0"
  },
  "scripts": {
    "prebuild": "sudo mkdir -p /run/secrets && sudo bash -c 'read -rsp \"Keystore password: \" KEYSTORE && echo \"$KEYSTORE\" > /run/secrets/pwd'",
    "build": "cd ./android && PASSWORD=$(cat /run/secrets/pwd) ./gradlew assembleRelease",
    "start": "yarn run run ",
    "poststart": "yarn run deploy",
    "deploy": "/vagrant/node_modules/.bin/react-native run-android",
    "prerun": "yarn run kill && adb reverse tcp:3030 tcp:3030",
    "run": "nohup /vagrant/node_modules/.bin/react-native start >/vagrant/react-native.log 2>&1 </dev/null &",
    "prekill": "watchman watch-del-all",
    "kill": "if [ \"$(pgrep -af '/vagrant/node_modules/.bin/react-native' | cut -d\" \" -f1)\" ]; then kill `pgrep -af '/vagrant/node_modules/.bin/react-native' | cut -d\" \" -f1`; fi",
    "log-native": "tail -f -n 50 /vagrant/react-native.log",
    "log-android": "adb logcat"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.2",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "3",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-react": "^7.1.0",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.44.0",
    "react-native-button": "^2.1.0",
    "react-native-checkbox": "^1.1.0",
    "react-native-hyperlink": "^0.0.10",
    "react-native-keyevent": "^0.0.5",
    "react-native-loading-spinner-overlay": "^0.5.1",
    "react-native-modalbox": "^1.4.1",
    "react-native-multi-slider": "https://github.com/joshjconlin/react-native-multi-slider",
    "react-native-radio-buttons": "https://github.com/ArnaudRinquin/react-native-radio-buttons",
    "react-native-sqlite-storage": "^3.3.2",
    "react-native-timer": "^1.3.1",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11"
  }
}
